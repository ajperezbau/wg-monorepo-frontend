{
  "name": "wg-monorepo-frontend",
  "version": "0.0.0",
  "packageManager": "npm@8.11.0",
  "workspaces": [
    "libs/*",
    "apps/*"
  ],
  "scripts": {
    "postinstall": "turbo run build",
    "test": "turbo run test",
    "lint": "eslint libs apps --ext .ts,.tsx,.vue --quiet --ignore-path .gitignore",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "prepare-release:stable": "auto shipit"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.9",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "auto": "^10.37.6",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.6.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "turbo": "^1.5.5",
    "typescript": "^4.7.4",
    "vue-eslint-parser": "^9.1.0"
  },
  "repository": "ajperezbau/wg-monorepo-frontend",
  "author": "Abraham <ajperezbau@protonmail.com>",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "conventional-commits",
      "first-time-contributor",
      "jira"
    ],
    "onlyPublishWithReleaseLabel": true
  }
}