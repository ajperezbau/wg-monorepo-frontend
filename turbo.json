{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.ts", "tests/**/*.ts"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts", "src/**/*.vue", "**/App.vue"]
    },
    "dev": {
      "outputs": []
    }
  }
}